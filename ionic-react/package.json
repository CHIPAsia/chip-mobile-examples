{
  "name": "chip-ionic-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-build": "sh -c 'if [ ! -f ./dist ]; then npm run build; fi'",
    "run:ios": "npm run check-build && ionic capacitor run ios -l --external",
    "run:android": "npm run check-build && ionic capacitor run android -l --external",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint"
  },
  "dependencies": {
    "@awesome-cordova-plugins/core": "^6.6.0",
    "@awesome-cordova-plugins/in-app-browser": "^6.6.0",
    "@capacitor/android": "5.6.0",
    "@capacitor/app": "5.0.7",
    "@capacitor/app-launcher": "^5.0.7",
    "@capacitor/browser": "^5.2.0",
    "@capacitor/core": "5.6.0",
    "@capacitor/haptics": "5.0.7",
    "@capacitor/ios": "5.6.0",
    "@capacitor/keyboard": "5.0.8",
    "@capacitor/status-bar": "5.0.7",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.6.7",
    "cordova-plugin-inappbrowser": "^6.0.0",
    "ionicons": "^7.0.0",
    "prettier": "prettier/prettier",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "5.6.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  },
